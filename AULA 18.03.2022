def fahrenheit(celsius):
    convertido = celsius * 9 / 5 + 32
    print(f"A temperatura {celsius} celsius convertida em fahrenheit é: {convertido}")
    return(convertido)
   
fahrenheit(25)

def quadrada(): #criei uma função chamada quadarada rque recebe um número
    num = int(input("Digite o valor: ")) #adiciona um valor pelo usuário
    raiz = num **2 # cálculo do valor informado
    return(raiz) #retorno da função criada

print(quadrada()) #chama a função


